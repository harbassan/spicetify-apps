{
  "compilerOptions": {
    "target": "ES2022",
    "jsx": "react",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ES2022",
      "DOM"
    ],
  },
  "include": [
    "./src/**/*"
  ]
}
